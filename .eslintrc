{
  "extends": "airbnb",
  "ecmaFeatures": {
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": [
    "import",
    "jsx-a11y",
    "react"
  ],
  "rules": {
    "import/no-dynamic-require": 0,
    "no-param-reassign": 0,
    "indent": 0,
    "no-tabs": 0,
    "generator-star-spacing": [
      "error",
      {
        "before": false,
        "after": true
      }
    ]
  }
}